{
  "name": "portable-client",
  "version": "1.0.0",
  "description": "Client to consume services",
  "main": "index.js",
  "author": "Paul Marquez",
  "contributors": [
    "Paul MÃ¡rquez"
  ],
  "keywords": [
    "innovations",
    "data science",
    "portable"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --colors --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js --colors --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "rimraf public",
    "clean:ubstall": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && rimraf node_modules public",
    "doc": "esdoc -c esdoc.json",
    "rimraf": "rimraf",
    "forever": "forever",
    "daemonize": "cross-env NODE_ENV=production forever start backend/",
    "lint": "npm run eslint \"src/**/*.js\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist && npm run lint",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "webpack-server": "webpack-dev-server --config config/webpack.dev.js --progress --colors --watch --content-base src/",
    "server:dev": "node index",
    "server:prod": "node backend/",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "eslint": "eslint",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
