    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "d3": "^3.5.17",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "forever": "^0.15.3",
    "http-proxy": "^1.16.2",
    "jquery": "^3.1.1",
    "lodash": "^4.16.4",
    "ng-file-upload": "^12.2.12",
    "ng-notify": "^0.7.1",
    "ng-table": "^2.1.0",
    "normalize.css": "^5.0.0",
    "pg": "^6.1.5",
    "uuid": "^3.0.1"
  },
